<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pe.gob.mobile.puntahermosa.puntahermosamobile">

    <application
        android:allowBackup="true"
        android:icon="@drawable/escudo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Login"
            android:label="@string/title_activity_login"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".LoginRegister" />
        <activity
            android:name=".MainMobile"
            android:label="@string/title_activity_main_mobile"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EventRegister"
            android:label="@string/title_activity_event_register"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>